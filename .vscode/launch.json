{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Object Detection (local PNG)",
  "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/run_object_detection.py",
      "console": "integratedTerminal",
      "args": [
        "--weights", "weights/best.pt",
        "--image", "data/images/mspc-naip-lax-airport.png",
        "--confidence", "0.2"
      ],
      "justMyCode": true
    },
    {
      "name": "Run Object Detection (GeoTIFF URL)",
  "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/run_object_detection.py",
      "console": "integratedTerminal",
      "args": [
        "--weights", "weights/best.pt",
        "--image", "https://naipeuwest.blob.core.windows.net/naip/v002/ca/2022/ca_060cm_2022/33118/m_3311823_nw_11_060_20220511.tif",
        "--confidence", "0.2",
        "--force-download",
        "-d", "5"
      ],
      "justMyCode": true
    },
    {
      "name": "Run Object Detection (GeoTIFF URL w/ Bbox)",
  "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/src/run_object_detection.py",
      "console": "integratedTerminal",
      "args": [
        "--weights", "weights/best.pt",
        "--image", "https://naipeuwest.blob.core.windows.net/naip/v002/ca/2022/ca_060cm_2022/33118/m_3311805_ne_11_060_20220511.tif",
        "--confidence", "0.2",
        "-d", "2",
        "--bbox", "[-118.4400,33.9250,-118.3720,33.9600]",
        "--out", "data/results/lax_bbox_results.png"
      ],
      "justMyCode": true
    },
    {
      "name": "Python: pytest (all)",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": ["-q"],
      "justMyCode": true
    },
    {
      "name": "Python: pytest current file",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": ["-q", "${file}"],
      "justMyCode": true
    }
  ]
}
